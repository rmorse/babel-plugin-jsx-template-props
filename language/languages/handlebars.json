
{
	"name": "handlebars",

	"replace": {
		"format": "{{[%_variable_]}}"
	},
	"list": {
		"open": "{{#[%_variable_]}}",
		"close": "{{/[%_variable_]}}",
		"objectProperty": "{{[%_variable_]}}",
		"primitive": "{{.}}"
	},
	"control": {
		"ifTruthy": {
			"open": "{{#if_truthy [%_variable_]}}",
			"close": "{{/if_truthy}}"
		},
		"ifFalsy": {
			"open": "{{#if_falsy [%_variable_]}}",
			"close": "{{/if_falsy}}"
		},
		"ifEqual": {
			"open": "{{#if_equal [%_variable_] [%_variable_]}}",
			"close": "{{/if_equal}}"
		},
		"ifNotEqual": {
			"open": "{{#if_not_equal [%_variable_] [%_variable_]}}",
			"close": "{{/if_not_equal}}"
		},
		"else": {
			"open": "{{else}}",
			"close": "{{/else}}"
		}
	}
}